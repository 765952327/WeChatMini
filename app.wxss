/*searchBtn  */

.top {
  padding: 10rpx 34rpx 10rpx 34rpx;
  background: #db4d6b;
  position: relative;
}

.search {
  background: #eaeae9;
  line-height: 40rpx;
  border-radius: 16px;
  width: 90%;
}

.search image {
  display: inline-block;
  width: 35rpx;
  height: 35rpx;
  padding: 15rpx 15rpx 15rpx 20rpx;
}

.search input {
  display: inline-block;
  height: 24rpx;
}
.toWrite {
  width:90rpx;
  height:90rpx;
  display: block;
  background-size:cover;
  position:fixed;
  right:74rpx;
  bottom:100rpx;
  border-radius: 50%;
  background-color: #cdcdcd;
  opacity: 0.7;
  z-index: 999;
  border: 2rpx white solid;
  box-shadow: 0rpx 0rpx 2rpx #f0f0f0;
}